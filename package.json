{
  "name": "cv-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "npx ts-node build.ts && next build",
    "start": "node -r module-alias/register ./dist --env=production",
    "lint": "next lint",
    "lint:server": "npx eslint -c server/.eslintrc.json --ext .ts server/src/",
    "lint:server-test":"npx eslint -c server/.eslintrc.json --ext .ts server/spec/",
    "test": "nodemon --config ./server/spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./server/spec"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.4",
    "jet-validator": "^1.0.4",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "jasmine": "^4.6.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "watch": [
      "./server/src"
    ],
    "ext": "ts",
    "ignore": [
      "./server/src/public"
    ],
    "exec": "./node_modules/.bin/ts-node tsconfig-dev.json --files -r tsconfig-paths/register ./server/src"
  },
  "_moduleAliases": {
    "@serv": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  }
}
